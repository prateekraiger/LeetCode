class Solution {
    public int countPrimes(int n) {
        int ans = 0;
        int[]array = new int[n];
        for(int i = 2;i<n;i++){
            if(array[i]==1)continue;
            ans++;
            for(int j = 1;j*i<n;j++){
                array[j*i]=1;
            }
        }
        return ans;
    }
}